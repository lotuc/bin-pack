{:deps true

 :dev-http {8080 "public"}

 :builds
 {:app
  {:target :browser
   :js-options {:entry-keys ["module" "browser" "main"]}
   :compiler-options {:infer-externs :auto
                      :output-feature-set :es6}
   :output-dir "public/js"
   :asset-path "js"
   :modules {:main {:init-fn lotuc.binpack.visualizer/start}}}}}
