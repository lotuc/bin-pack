{
  "name": "bin-pack-visualizer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postcss:build": "cross-env postcss src/main/tailwind.css -o ./public/css/main.css --verbose",
    "postcss:watch": "cross-env postcss src/main/tailwind.css -o ./public/css/main.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss src/main/tailwind.css -o ./public/css/main.css --verbose",
    "shadow:watch": "shadow-cljs watch frontend",
    "shadow:release": "shadow-cljs release frontend",
    "dev": "run-p -l *:watch",
    "release": "run-s *:release"
  },
  "devDependencies": {
    "shadow-cljs": "2.23.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2"
  }
}
